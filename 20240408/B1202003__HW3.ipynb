        "year = int(input(\"請輸入年份：\"))\n",
        "\n",
        "if year <= 1582:\n",
        "    if (year % 4 == 0):\n",
        "        print(\"閏年\")\n",
        "    else:\n",
        "        print(\"平年\")\n",
        "\n",
        "else:\n",
        "    if (year % 4 == 0 and year % 100 != 0) or (year % 400 == 0):\n",
        "        print(\"閏年\")\n",
        "    else:\n",
        "        print(\"平年\")\n"
